{
  "name": "vue-product-carousel",
  "version": "0.2.0",
  "description": "Simple slider on Vue.js with hot image replacement, Zoom and Swipe mode",
  "author": "Max Lyashuk <m_lyashuk@ukr.net>",
  "private": false,
  "main": "dist/vue-product-carousel.min.js",
  "scripts": {
    "dev": "webpack-dev-server --inline --hot --config build/webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules --config build/webpack.config.js",
    "dist": "cross-env NODE_ENV=production webpack --progress --hide-modules --config build/dist.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/probil/vue-product-carousel.git"
  },
  "devDependencies": {
    "vue": "^1.0.0",
    "babel-runtime": "^6.0.0",
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "cross-env": "^2.0.1",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "url-loader": "^0.5.7",
    "vue-hot-reload-api": "^1.2.0",
    "vue-html-loader": "^1.0.0",
    "vue-loader": "^8.5.2",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/probil/vue-product-carousel/blob/master/LICENSE"
    }
  ]
}
